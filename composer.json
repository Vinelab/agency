{
	"name": "Agency/central",
	"description": "",
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"vinelab/social-auth": "3.1.*",
		"intervention/image": "2.*",
		"aws/aws-sdk-php": "2.*",
		"vinelab/neoeloquent":"1.2.*",
		"guzzlehttp/guzzle" : "~5.0",
		"vinelab/api-manager": "0.7.*",
		"vinelab/cdn": "1.1.*",
		"predis/predis": "1.*",
		"illuminate/html": "5.*",
		"vinelab/minion": "1.3.*",
        "vinelab/youtube" : "0.9.*",
        "vinelab/laravel-editor": "1.*",
        "vinelab/url-shortener": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "4.*",
		"mockery/mockery": "0.9.*",
		"fzaninotto/faker": "dev-master"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Agency\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php",
			"tests/ApiTestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"stability": "stable"
}
