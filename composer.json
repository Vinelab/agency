{
	"name": "Vinelab/agency",
	"description": "A backend content management system built with smart tools and compelling UI includes an API built with mobile in mind.",
	"keywords": ["cms", "api", "laravel", "aws", "alc"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
		"vinelab/social-auth": "3.*",
		"intervention/image": "dev-master",
		"aws/aws-sdk-php": "2.*"
	},
	"require-dev": {
		"phpunit/phpunit": "3.*",
		"mockery/mockery": "0.9.*"
	},
	"autoload": {
		"psr-0": {
			"Agency": "app/"
		},
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
